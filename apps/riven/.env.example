# PostgreSQL database connection URL
DATABASE_URL=

# Redis connection URL
REDIS_URL=

# Real-Debrid API Key
REAL_DEBRID_API_KEY=

# Listrr API Key
LISTRR_API_KEY=

# gRPC VFS Configuration

# Enable the gRPC VFS server (default: true)
ENABLE_GRPC_VFS=true

## Server address and port for gRPC VFS (default: 0.0.0.0:50051)
GRPC_VFS_SERVER_ADDR=0.0.0.0:50051

## Default chunk size for file streaming in bytes (default: 1048576 = 1MB)
GRPC_VFS_CHUNK_SIZE=1048576

